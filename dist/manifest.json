{
  "manifest_version": 2,
  "name": "ENS Content Resolver",
  "description": "Resolves any .eth address that has an IPFS hash registered as content and redirects your browser to the content.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "res/ens_512x512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": {
    "512": "res/ens_512x512.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "*://*.eth/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "*"
  ]
}
